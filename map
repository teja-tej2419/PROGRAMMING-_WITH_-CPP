#include <iostream>
#include <vector>
#include <list>
#include <map>
using namespace std;

int main() {
    vector<int> v = {1, 2, 3, 4, 5};
    v.push_back(6);
    v.erase(v.begin() + 2);
    for (int i : v) {
        cout << "Vector: " << i << endl;
    }

    list<string> l = {"apple", "banana", "cherry"};
    l.push_front("mango");
    l.remove("banana");
    for (const string& s : l) {
        cout << "List: " << s << endl;
    }

    map<string, int> m;
    m["Alice"] = 25;
    m["Bob"] = 30;
    m["Charlie"] = 28;
    m.erase("Bob");
    for (auto& pair : m) {
        cout << "Map: " << pair.first << " = " << pair.second << endl;
    }

    return 0;
}
